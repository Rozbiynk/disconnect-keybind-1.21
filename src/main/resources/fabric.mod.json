{
  "schemaVersion": 1,
  "id": "disconnect",
  "version": "${version}",
  "name": "Disconnect Keybind",
  "license": "MIT",
  "icon": "assets/icon.png",
  "description": "Bind yourself a button to Disconnect!",
  "authors": [
    "Shateq"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/disconnect",
    "sources": "https://github.com/shateq/disconnect-keybind",
